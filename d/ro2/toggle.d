import std.file, std.stdio;

void toggleCrypt(ubyte[] data) {
	static const char[] xor = x"";
	for (int n = 0; n < data.length; n++) data[n] ^= xor[n % xor.length];
}

void toggleCryptFile(char[] name) {
	ubyte[] data = cast(ubyte[])read(name);
		toggleCrypt(data);
	write(name, data);	
}

int main(char[][] args) {
	if (args.length < 2) {
		writefln("toggle <file.dat>");
	} else {
		toggleCryptFile(args[1]);
	}
	
	return 0;
}